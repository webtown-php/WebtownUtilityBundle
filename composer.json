{
    "name": "webtown/utility-bundle",
    "type": "symfony-bundle",
    "description": "Utility bundle",
    "keywords": [
    ],
    "license": "MIT",
    "authors": [
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "symfony/symfony": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0"
    },
    "suggest": {
        "stof/doctrine-extensions-bundle": "Timestampable trait",
        "doctrine/orm": "Key => Value hydrator"
    },
    "autoload": {
        "psr-4": {
            "Webtown\\UtilityBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webtown\\UtilityBundle\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
